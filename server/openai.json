                    },
                    "default": {
                        "description": "Error response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/GenericErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/learningPath/update/title": {
            "post": {
                "description": "Update a learning paths title.",
                "responses": {
                    "200": {
                        "description": "Success response",
                        "content": {
                            "application/json": {
                                "schema": {
                                   "$ref": "#/components/schemas/GenericSuccessResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Error response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/GenericErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/learningPathStop/update/title": {
            "post": {
                "description": "Update Learning Path stops title.",
                "responses": {
                    "200": {
                        "description": "Success response",
                        "content": {
                            "application/json": {
                                "schema": {
                                   "$ref": "#/components/schemas/GenericSuccessResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Error response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/GenericErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/learningPathStop/update/body": {
            "post": {
                "description": "Update Learning Path stops body.",
                "responses": {
                    "200": {
                        "description": "Success response",
                        "content": {
                            "application/json": {
                                "schema": {
                                   "$ref": "#/components/schemas/GenericSuccessResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Error response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/GenericErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/learningPathStop/create": {
            "post": {
                "description": "Create a learning path stop.",
                "responses": {
                    "200": {
                        "description": "Success response",
                        "content": {
                            "application/json": {
                                "schema": {
                                   "$ref": "#/components/schemas/GenericSuccessResponse"
                                }
                            }
                        }
                    },
                    "default": {
                        "description": "Error response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/GenericErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}